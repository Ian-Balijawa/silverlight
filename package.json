{
  "name": "silver-light",
  "repository": {
    "url": "https://github.com/Ian-Balijawa/silverlight.git",
    "type": "git"
  },
  "displayName": "Silver Light",
  "description": "VSCode Silver Theme",
  "publisher": "ianbalijawa",
  "version": "0.2.13",
  "scripts": {
    "start": "node ./src/addColors.js",
    "build": "node ./src/addColors.js && build-theme && vsce package",
    "pub": "git add . && git commit -m 'refactors'  && vsce publish && git push"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "Light",
    "Dark",
    "Silver",
    "Gray",
    "Theme",
    "Themes",
    "Calm",
    "Pale",
    "Focusing",
    "Concentrate",
    "An Old Hope",
    "Github",
    "Usher",
    "Noctis",
    "One Dark"
  ],
  "icon": "icon.png",
  "contributes": {
    "themes": [
      {
        "id": "Silver Light",
        "label": "Silver Light",
        "uiTheme": "vs",
        "path": "./output/silver-gray-color-theme.json"
      },
      {
        "id": "Silver Dark",
        "label": "Silver Dark",
        "uiTheme": "vs-dark",
        "path": "./output/silver-dark-color-theme.json"
      },
      {
        "id": "Silver Gad",
        "label": "Silver Gad",
        "uiTheme": "vs-dark",
        "path": "./output/silver-Gad-color-theme.json"
      },
      {
        "id": "Silver Usher",
        "label": "Silver Usher",
        "uiTheme": "vs",
        "path": "./output/silver-usher-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@two-beards/vscode-theme-builder": "^1.0.0",
    "deltree": "^1.2.8",
    "write": "^2.0.0"
  }
}
